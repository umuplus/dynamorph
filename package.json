{
    "name": "dynamorph",
    "version": "0.1.0-development",
    "description": "a minimalist toolkit for aws dynamodb",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "lint": "eslint './src/**/*.{js,ts,tsx}'",
        "test": "ava",
        "test:clean": "npm run clean && npm test && npm run clean",
        "test:coverage": "nyc ava",
        "build": "tsc",
        "watch": "tsc -w",
        "clean": "rm -rf dist coverage tmp"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/umuplus/dynamorph.git"
    },
    "keywords": [
        "aws",
        "dynamodb",
        "ddb"
    ],
    "author": "Umut Aydin",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/umuplus/dynamorph/issues"
    },
    "homepage": "https://github.com/umuplus/dynamorph#readme",
    "devDependencies": {
        "@types/node": "18.11.7",
        "@types/sinon": "10.0.13",
        "@typescript-eslint/eslint-plugin": "5.41.0",
        "@typescript-eslint/parser": "5.41.0",
        "aws-sdk-client-mock": "2.0.0",
        "ava": "5.0.1",
        "nyc": "15.1.0",
        "eslint": "8.26.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-prettier": "4.2.1",
        "prettier": "2.7.1",
        "sinon": "14.0.1",
        "ts-node": "10.9.1",
        "typescript": "4.8.4"
    },
    "files": [
        "dist/**/*"
    ],
    "dependencies": {
        "@aws-sdk/client-dynamodb": "3.197.0",
        "@aws-sdk/lib-dynamodb": "3.197.0",
        "radash": "9.0.2",
        "ulid": "2.3.0"
    }
}
